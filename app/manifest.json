{
  "manifest_version": 2,
  "name": "Offline Player",
  "version": "0.0.1",
  "minimum_chrome_version": "41",
  "permissions": ["webview", "http://localhost:8080/*"],
  "app": {
    "background": {
      "scripts": ["background-script.js"]
    }
  },
  "permissions": [
    "geolocation",
    "power",
    "storage",
    "unlimitedStorage",
    "system.display"
  ]
}
